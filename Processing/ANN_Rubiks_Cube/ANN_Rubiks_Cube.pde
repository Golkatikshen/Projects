void setup()
{
  size(1100, 830);
  textFont(createFont("Consolas", 25));
  
  rg = new RubikGrid(30, 30, 70, 3);
  scramble_mem = "";
  
  ann = new ANNetwork(0, 0, 162, new int[] {120, 60}, 12);
  wave = new Wave(10, 820, 1080);
  
  R = new Button(990, 30, 30, 30, "R", 6, -1,  100, 100, 100);
  L = new Button(1030, 30, 30, 30, "L", 6, -1,  100, 100, 100);
  U = new Button(1070, 30, 30, 30, "U", 6, -1,  100, 100, 100);
  D = new Button(990, 70, 30, 30, "D", 6, -1,  100, 100, 100);
  F = new Button(1030, 70, 30, 30, "F", 6, -1,  100, 100, 100);
  B = new Button(1070, 70, 30, 30, "B", 6, -1,  100, 100, 100);
}

void draw()
{
  background(0);
  fill(0, 255, 0);
  textSize(14);
  textAlign(LEFT);
  text("FPS: " + (int)frameRate, 5, 15);
  
  if(!rg.retCoordIdMouseHover()[0].equals(""))
  {
    textSize(16);
    text("Coord: " + rg.retCoordIdMouseHover()[0], 5, 40);
    text("ID: " + rg.retCoordIdMouseHover()[1], 5, 60);
  }
  
  
  if(!scramble_mem.equals(""))
  {
    textSize(25);
    text("Scramble:  " + scramble_mem, 500, height-140);
  }
  
  rg.display();
  wave.display();
  R.display();
  L.display();
  U.display();
  D.display();
  F.display();
  B.display();
  
  
  if(train)
    trainN();
}


