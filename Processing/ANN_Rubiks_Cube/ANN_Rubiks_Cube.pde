void keyPressed()
{
  if(key == 's')
  {
    ann.annSave("ann1.txt");
    println("saved.");
  }
  
  if(key == 't')
    train = !train;
    
  if(key == 'p')
    rg.printGrid();
  
  if(key == 'q')
  {
    float[] o = ann.forward(rg.retGridPieceFocus(new int[] {7, 19}));
    float max = -10;
    int i_max = 0;
    
    for(int i=0; i<o.length; i++)
      if(max < o[i])
      {
        max = o[i];
        i_max = i;
      }
    
    printArray(o);
    rg.exeMove(i_max);
  }
  
  
  if(key == 'r')
    rg.reset();
  
  if(key == 'c')
    rg.setColors();
    
  if(key == 'x')
    rg.scramble(8);
    
    
  if(key == CODED)
    if(keyCode == SHIFT)
      shift = true;
}

void keyReleased()
{
  if(key == CODED)
    if(keyCode == SHIFT)
      shift = false;
}


void mousePressed()
{
  if(mouseButton == LEFT)
    rg.clickQuadCheck(1);
  if(mouseButton == RIGHT)
    rg.clickQuadCheck(-1);
    
  
  if(R.mouseHover())
    if(shift)
      rg.Ri();
    else
    rg.R();
    
  if(L.mouseHover())
    if(shift)
      rg.Li();
    else
    rg.L();
    
  if(U.mouseHover())
    if(shift)
      rg.Ui();
    else
    rg.U();
    
  if(D.mouseHover())
    if(shift)
      rg.Di();
    else
    rg.D();
    
  if(F.mouseHover())
    if(shift)
      rg.Fi();
    else
    rg.F();
    
  if(B.mouseHover())
    if(shift)
      rg.Bi();
    else
    rg.B();
}